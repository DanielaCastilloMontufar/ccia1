---
import FooterSection from "../components/FooterSection.astro";
import LandingHeader from "../components/LandingHeader.astro";
import Arrow1 from "../components/comuns/Arrow1.astro";
import Formulario from "../components/comuns/Formulario.astro";
import Layout from "../layouts/Layout.astro";

---

<Layout title="Organización de Webinars">
    <LandingHeader/>
    <section class="orgWeb pt-40">
        <div class="dtl grid grid-cols-2 gap-2">
            <div class="pl-10">
                <h2>Organización de <span><br />Webinars</span></h2>
                <p>
                    Organiza seminarios virtuales, personalizados y gratuitos.<br /><br />
                    Si dominas un tema en particular y quieres que te conozcan, este servicio es para ti.<br /><br />
                    Facilitamos la difusión de tu evento a través de nuestros canales digitales.
                </p>
    
                <a href="#certificados-origen"><button type="submit" class="btn btn-secundario flex mt-10"
                    ><i> <Arrow1/></i> Ver más</button
                ></a>
            </div>
    
            <div>
                <div class="imagen1">
                    <img
                        class="app-img1"
                        src="./images/webinars.webp"
                        alt=""
                    />
                </div>
                <div class="flex">
                    <div class="w-1/2"><a href="/organizacionwebinars/"><img
                    class=" pt-10 w-200"
                    src="./images/videos.webp"
                    alt=""
                /></a></div>
                <div class="pt-10 w-1/2">
                    <p class="uppercase">Novedades</p>
                    <p class="">¿Qué es el libro de reclamaciones?</p>
                </div></div>
            </div>
        </div>
        
